<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="201.0" prefWidth="245.0" spacing="20" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.SettingsController">
    <children>
        <Label text="Instellingen" />
        <ComboBox fx:id="languageDropdown" />
        <ToggleButton fx:id="themeToggle" text="Toggle Theme" />
      <Button fx:id="editUser" mnemonicParsing="false" onAction="#navEditUser" prefHeight="26.0" prefWidth="356.0" text="username/wachtwoord aanpassen" />
        <Button fx:id="backButton" onAction="#goBack" text="Terug" />
    </children>
</VBox>
